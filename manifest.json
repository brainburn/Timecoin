{
    "manifest_version": 2,
    "name": "Timecoin",
    "description": "Converts money back to time",
    "version": "1.0",
    "content_scripts": [
        {
            "matches": [
                "http://*/*",
                "https://*/*"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_end"
        }
    ]
}